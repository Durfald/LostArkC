<Window x:Class="LostArkManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LostArkManager"
        xmlns:extensions="clr-namespace:LostArkManager.LOSTARK.Extensions"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="915">
    <Grid x:Name="MG" Background="#222222">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="7*"/>
            <ColumnDefinition Width="33*"/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <Button Style="{StaticResource LeftButtonStyle}" Content="Гайды"/>
            <Button Style="{StaticResource LeftButtonStyle}" Content="Билды"/>
            <Button Style="{StaticResource LeftButtonStyle}" Content="Персонажи"/>
            <Button Style="{StaticResource LeftButtonStyle}" Content="Топ"/>
        </StackPanel>
        <!--<Button Style="{StaticResource BingoSkullButtonStyle}" extensions:ImageButton.ImageSource="{StaticResource Skull}" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="71" Width="67" Margin="323,185,0,0"/>-->
    </Grid>
</Window>